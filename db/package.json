{
  "config": {
    "env_dev": "node --env-file=.env",
    "env_test": "node --env-file=.env.test",
    "env_prod": "node --env-file=.env.production",
    "cli_dev": "node --env-file=.env node_modules/.bin",
    "cli_test": "node --env-file=.env.test node_modules/.bin",
    "cli_prod": "node --env-file=.env.production node_modules/.bin"
  },
  "scripts": {
    "build": "tsc",
    "generate": "npx drizzle-kit generate",
    "push": "npx drizzle-kit push",
    "check": "npx drizzle-kit check",
    "seed": "npx tsx src/seed/seed.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npx drizzle-kit migrate",
    "migrate:prod": "npx drizzle-kit migrate",
    "studio": "npx drizzle-kit studio",
    "studio:dev": "$npm_package_config_cli_dev/drizzle-kit studio",
    "studio:test": "$npm_package_config_cli_test/drizzle-kit studio",
    "studio:prod": "$npm_package_config_cli_prod/drizzle-kit studio"
  },
  "main": "dist/index.js",
  "description": "",
  "dependencies": {
    "drizzle-orm": "^0.44.2",
    "pg": "^8.16.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.48.4",
    "@types/pg": "^8.15.4",
    "drizzle-kit": "^0.31.1",
    "tsx": "^4.20.3"
  }
}
